import serial 
import time 
import binascii 

ser1 = serial.Serial("/dev/ttyTHS2",100000,timeout=1.5)

msg = '04205b2ea8'
# ser1.write(b)
print(msg)
# msg = [0]+[0x1]*16
msg=[0x0,0x0,0x0,0x05,0x20,0x5b,0x2e,0xa8,0x0,0x0,0x0]
msg = '0'+'1'*17
msg = [0]*17+[1]
msg = '0'*17+'1'

msg=[0x0,0x05,0x20,0x5b,0x2e,0xa8,0x0] + [0x0]*11
msg = [0x42,0x05,0xb3,0x2a,0x80] + [0]*13
msg = [0,0x04,0xa0,0xff,0x00,0xe8] + [0]*12

print(msg)
while True:
	ser1.write(msg)
	time.sleep(.1)
